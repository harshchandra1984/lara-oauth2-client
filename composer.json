{
    "name": "larawizards/lara-oauth2-client",
    "description": "Laravel OAuth2 client package for single sign-on (SSO) authentication with Fortify/Jetstream integration",
    "type": "library",
    "keywords": [
        "laravel",
        "oauth2",
        "sso",
        "single-sign-on",
        "authentication",
        "fortify",
        "jetstream"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Harsh Chandra",
            "email": "harshchandra@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0|^11.0|^12.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Larawizards\\LaraOAuth2Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larawizards\\LaraOAuth2Client\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larawizards\\LaraOAuth2Client\\LaraOAuth2ClientServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
